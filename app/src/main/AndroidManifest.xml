<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="CoarseFineLocation">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.PROXIMITY_SCREEN_OFF_WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="com.mapbox.maps.location.gms.SKIP_IF_INVALID_VERSION"
        android:value="true" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mapbox"
        tools:targetApi="31">
        <activity
            android:name=".FB_LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MQTTActivity"
            android:exported="false" />
        <activity
            android:name=".FB_RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".ReadActivity"
            android:exported="false" />
        <activity
            android:name=".RegistroActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MapaActivity"
            android:exported="true" />
        <activity
            android:name=".SensorActivity"
            android:exported="true" />
        <activity
            android:name=".Editar"
            android:exported="true" />
        

        
    </application>

</manifest>